<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.will.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/SOS_Help_App"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HelpActivity" />
        <activity
            android:name=".ProfilePage"
            android:label="@string/title_activity_profile_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".JoinGroup"
            android:label="@string/title_activity_join_group"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".createGroup"
            android:label="@string/title_activity_create_group"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>